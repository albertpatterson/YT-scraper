{
  "manifest_version": 3,
  "name": "youtube_recommendations_scraper",
  "version": "1.0.0",

  "description": "Scrape Youtube recommendations - made with create-extension",

  "action": {
    "default_title": "YT-Scraper",
    "default_icon": {
      "16": "icon/icon16.png",
      "24": "icon/icon24.png",
      "32": "icon/icon32.png"
    }
  },
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "background": {
    "service_worker": "background/service_worker.js"
  },
  "commands": {},
  "homepage_url": "https://github.com/albertpatterson/youtube-recommendations-scraper",
  "permissions": ["activeTab", "scripting"]
}
